{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm install in vite-project",
      "type": "shell",
      "command": "npm install",
      "options": {
        "cwd": "${workspaceFolder}/vite-project"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "npm run dev in vite-project",
      "type": "shell",
      "command": "npm run dev",
      "options": {
        "cwd": "${workspaceFolder}/vite-project"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
  ]
}
